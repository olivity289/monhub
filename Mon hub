local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("mon hub ", "GrapeTheme")

-- Hub

local Hub = Window:NewTab ("Hub")

local HubSection = Hub:NewSection("Hub")

HubSection:NewButton("Maxilitoscripthub", " netless", function()

    loadstring(game:HttpGet ("https://raw.githubusercontent.com/olivity289/Maxilitoscripthub/main/Maxilitoscripthub"))();

end)

HubSection:NewButton("GelatekHub", " GelatekHub", function()

    loadstring(game:HttpGet("https://raw.githubusercontent.com/toldblock/GelatekHub/main/Main.lua"))()

end)

HubSection:NewButton("olidragon210hubv1", " olidragon210hubv1", function()

    loadstring(game:HttpGet("https://raw.githubusercontent.com/olidragon210/olidragon210hubv1.1/main/olidragon210hubv1.1"))();

end)

HubSection:NewButton("Fe chat translator", " Google traduction", function()

    loadstring(game:HttpGet ("https://raw.githubusercontent.com/olivity289/chat-translator/main/Chat%20translator"))();

end)
